{
  "name": "SEOutline",
  "version": "1.0",
  "description": "Voir rapidement les éléments de référencement",
  "background_page": "background.html",
  "permissions": [
    "tabs", "http://*/*"
  ],
  "browser_action": {
    "name": "Outline SEO-oriented HTML tags",
    "default_icon": "html.png",
    "default_title": "SEOutline elements",
    "icons": ["html.png"]
  },
  "content_scripts": [{}],
  "content_security_policy": "default-src 'none'; script-src 'self'"
}

